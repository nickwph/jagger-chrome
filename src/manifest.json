{
    "manifest_version": 2,

    "name": "Jaeger",
    "version": "4.0.0",
    "description": "Inject Javascripts to Any Websites.",
    "icons": {
        "16": "image/icon.png",
        "48": "image/icon.png",
        "128": "image/icon.png"
    },


    "background": {
        "page": "view/background.html"
    },

    "options_page": "view/options.html",

    "browser_action": {
        "default_icon": "image/icon.png",
        "default_title": "JsInjector",
        "default_popup": "view/popup.html"
    },

    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],

    "update_url": "http://clients2.google.com/service/update2/crx"
}